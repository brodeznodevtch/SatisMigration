{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "automattic/woocommerce": "^2.0",
        "barryvdh/laravel-dompdf": "^0.8.6",
        "composer/semver": "^1.4",
        "consoletvs/charts": "^5.4",
        "doctrine/dbal": "^2.10",
        "guzzlehttp/guzzle": "^7.7",
        "iatstuti/laravel-cascade-soft-deletes": "1.5.0",
        "knox/pesapal": "^1.3",
        "laravel/framework": "5.8.*",
        "laravel/tinker": "~1.0",
        "laravelcollective/html": "5.8.*",
        "mike42/escpos-php": "^3.0",
        "milon/barcode": "^5.3",
        "nwidart/laravel-modules": "^3.3",
        "razorpay/razorpay": "2.*",
        "spatie/flysystem-dropbox": "^1.0",
        "spatie/laravel-backup": "^5.7",
        "spatie/laravel-permission": "^2.7",
        "srmklive/paypal": "^1.6",
        "stripe/stripe-php": "^6.10",
        "yajra/laravel-datatables-buttons": "^4.10",
        "yajra/laravel-datatables-oracle": "^8.3",
        "fideloper/proxy": "^4.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.1",
        "deployer/deployer": "^7.3",
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^7.5",
        "theanik/laravel-more-command": "^1.3",
        "nunomaduro/collision": "^3.0",
        "beyondcode/laravel-dump-server": "^1.0"
    },
    "autoload": {
        "files": [
            "app/Http/helpers.php"
        ],
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/",
            "Modules\\": "Modules/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
